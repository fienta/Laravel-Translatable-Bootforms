{
    "name": "fienta/laravel-translatable-bootforms",
    "description": "Empowers adamwathan/bootforms with dimsav/laravel-translatable.",
    "keywords": ["laravel", "bootforms", "translatable", "i18n", "form"],
    "license": "MIT",
    "authors": [
	{
	    "name": "Fienta",
	    "email": "hello@fienta.com"
	}
    ],
    "require": {
        "php": ">=5.5.9|>=8.2.0",
        "illuminate/support": ">=5.5|>=11.0",
        "fienta/bootforms": "^1.2.0|^1.7.2",
        "astrotomic/laravel-translatable": ">=8.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=3.0",
        "phpunit/phpunit": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TypiCMS\\LaravelTranslatableBootForms\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
